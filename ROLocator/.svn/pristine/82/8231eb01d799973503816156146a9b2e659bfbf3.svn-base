<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>MapLocations</servlet-name>
        <servlet-class>GlobalData.MapLocations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NearLocations</servlet-name>
        <servlet-class>GlobalData.NearLocations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HighwayRO</servlet-name>
        <servlet-class>GlobalData.HighwayRO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>promodetails</servlet-name>
        <servlet-class>Promo.promodetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StateWiseLocator</servlet-name>
        <servlet-class>GlobalData.StateWiseLocator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DistrictWiseLocator</servlet-name>
        <servlet-class>GlobalData.DistrictWiseLocator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MapLocations</servlet-name>
        <url-pattern>/MapLocations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NearLocations</servlet-name>
        <url-pattern>/NearLocations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HighwayRO</servlet-name>
        <url-pattern>/HighwayRO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>promodetails</servlet-name>
        <url-pattern>/promodetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StateWiseLocator</servlet-name>
        <url-pattern>/StateWiseLocator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DistrictWiseLocator</servlet-name>
        <url-pattern>/DistrictWiseLocator</url-pattern>
    </servlet-mapping>
<!--    <servlet>
        <servlet-name>ErrorHandler</servlet-name>
        <jsp-file>/exception.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>ErrorHandler</servlet-name>
        <url-pattern>/ErrorHandler</url-pattern>
    </servlet-mapping>-->
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>location.jsp</welcome-file>
    </welcome-file-list>
